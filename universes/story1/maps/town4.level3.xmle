		// paths
        npc("npc1").addHotspot("npc1.a")
                   .addHotspot("sleep(0.75)")
                   .addHotspot("npc1.b")
                   .addHotspot("sleep(0.5)")
                   .addHotspot("npc1.c")
                   .addHotspot("sleep(1.25)")
                   .addHotspot("npc1.d")
                   .addHotspot("npc1.e")
                   .addHotspot("sleep(0.75)")
                   .addHotspot("npc1.f")
                   .addHotspot("sleep(1.0)");

        // trigger follow
        trigger("npc1.talk").follow("npc1");

        // paths
        npc("npc2").addHotspot("npc2.a")
                   .addHotspot("sleep(0.5)")
                   .addHotspot("npc2.b")
                   .addHotspot("npc2.c")
                   .addHotspot("sleep(0.75)")
                   .addHotspot("npc2.b")
                   .addHotspot("sleep(0.5)")
                   .addHotspot("npc2.d")
                   .addHotspot("sleep(1.25)")
                   .addHotspot("npc2.b");

        // trigger follow
        trigger("npc2.talk").follow("npc2");
	</script>
</scripts>
