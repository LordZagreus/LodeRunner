<scripts>
	<script name = 'onload'>
		map()

            .setParams({
                "overview": "Sometimes you should ignore a lever entirely, especially if it crushes you.",
                "tooltip": "Use your [color=special]mining laser[/color] liberally!",
                "prizes.warehouses": ["puzzle1"],
                "prizes.min-quality": 1,
                "prizes.max-quality": 1,
                "prizes.required-item-names": ["running-shoes"]
            })

            .setWaveAllowances({
                "bombs": 0
            });

        showPuzzleIntro();
	</script>
	<script name = 'shift1.shift.x'>
		if ( npc("lever.x1").hasPosition("left") )
        {
            npc("lever.x1").setPosition("right");
            npc("lever.x2").setPosition("right");
            map().plane("shift1").shiftToX("shift1.x2", 2.0);
        }

        else
        {
            npc("lever.x1").setPosition("left");
            npc("lever.x2").setPosition("left");
            map().plane("shift1").shiftToX("shift1.x1", 2.0);
        }
	</script>
	<script name = 'shift1.shift.y'>
		if ( npc("lever.y1").hasPosition("up") )
        {
            npc("lever.y1").setPosition("down");
            npc("lever.y2").setPosition("down");
            map().plane("shift1").shiftToY("shift1.y2", 2.0);
        }

        else
        {
            npc("lever.y1").setPosition("up");
            npc("lever.y2").setPosition("up");
            map().plane("shift1").shiftToY("shift1.y1", 2.0);
        }
	</script>
