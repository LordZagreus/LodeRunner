	<script name = 'lever1.action'>
		// lever set down?
        if ( npc("lever1").hasPosition("down") )
        {
            // raise lever
            npc("lever1").setPosition("up");

            // shift platform
            map().plane("shift1").shiftToY("shift1.top");
        }

        else
        {
            // lower lever
            npc("lever1").setPosition("down");

            // shift platform
            map().plane("shift1").shiftToY("shift1.bottom");
        }
	</script>
